{
  "name": "mdir.js",
  "version": "1.2.7-beta+058",
  "description": "Mdir.js is visual file manager. (text mode application)",
  "main": "./build/main.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "tsc -p tsconfig.json",
    "start": "tsc -p tsconfig.json && node ./build/main.js",
    "debug": "tsc -p tsconfig.json && node --inspect-brk ./build/main.js",
    "jest": "jest",
    "test": "tsc -p tsconfig.json && node ./build/test/test.js",
    "testimage": "tsc -p tsconfig.json && node ./build/test/imagetest.js",
    "testeditor": "tsc -p tsconfig.json && node ./build/test/editortest.js",
    "archivetest": "tsc -p tsconfig.json && node ./build/test/archive.js",
    "colordemo": "tsc -p tsconfig.json && node ./build/test/colordemo.js",
    "prepare": "tsc -p tsconfig.json",
    "postinstall": "node ./scripts/post-install.js",
    "pkg-linux": "pkg --target=node12-linux-x64 --out-path=./binary .",
    "pkg-osx": "pkg --target=node12-macos-x64 --out-path=./binary .",
    "pkg-win": "pkg --target=node12-win-x64 --out-path=./binary .",
    "predocker": "npm pack",
    "docker": "docker-compose build"
  },
  "author": "La Byoungyoung <la9527@daum.net>",
  "bugs": {
    "url": "https://github.com/la9527/mdir.js/issues",
    "email": "la9527@daum.net"
  },
  "contributors": [
    {
      "name": "La Byoungyoung",
      "email": "la9527@daum.net"
    }
  ],
  "bin": {
    "mdir": "bin/mdir"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/la9527/mdir.js.git"
  },
  "license": "BSD3",
  "keywords": [
    "MDir",
    "MC",
    "filemanager",
    "LinM",
    "CUI",
    "Xterm",
    "ssh",
    "sftp"
  ],
  "engines": {
    "node": ">= 10.20.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "colors": "^1.4.0",
    "file-type": "^14.6.2",
    "fswin": "^3.18.918",
    "i18next": "^19.5.1",
    "i18next-cli-language-detector": "^1.0.0",
    "i18next-fs-backend": "^1.0.6",
    "iconv-lite": "^0.6.2",
    "jimp": "^0.14.0",
    "jschardet": "^2.2.1",
    "mime-types": "^2.1.27",
    "moment": "^2.26.0",
    "neo-blessed": "^0.2.0",
    "node-machine-id": "^1.1.12",
    "node-pty-prebuilt-multiarch": "^0.9.0",
    "node-stream-zip": "^1.11.2",
    "os-locale": "^5.0.0",
    "socks": "^2.4.1",
    "sprintf-js": "^1.1.2",
    "ssh2": "^1.5.0",
    "supports-color": "^7.1.0",
    "tar-fs": "^2.1.0",
    "tar-stream": "^2.1.3",
    "term.js": "0.0.7",
    "unbzip2-stream": "^1.4.3",
    "unzip-stream": "^0.3.0",
    "which": "^2.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "~3.8.0",
    "xterm": "4.7.0",
    "yargs": "^15.3.1",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1",
    "zip-stream": "^4.0.2"
  },
  "optionalDependencies": {
    "lzma-native": "6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.54",
    "@types/sprintf-js": "^1.1.2",
    "@types/tar-stream": "^2.1.0",
    "@types/yargs": "^15.0.5",
    "@types/yauzl": "^2.9.1",
    "@types/yazl": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "node-pty": "^0.9.0",
    "node-gyp": "^7.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "pkg": "^4.4.8",
    "typescript": "^3.9.3"
  },
  "pkg": {
    "scripts": [
      "build/**/*.js"
    ],
    "assets": [
      "node_modules/neo-blessed/**"
    ],
    "tagets": [
      "node12"
    ]
  }
}
